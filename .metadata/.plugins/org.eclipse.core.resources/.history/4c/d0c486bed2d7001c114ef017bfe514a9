package com.gabrielsantos.exerciciosalgoritmosbasicos;

import java.util.Scanner;

public class Exercicio5 {

	public static void main(String[] args) {
		/**
		 * Elaborar um algoritmo que lê 2 valores a e b e os escreve com a mensagem:
		 * "São múltiplos" ou "Não são múltiplos".
		 */
		Scanner entradaUsuario = new Scanner(System.in);
		int primeiroValor = 0, segundoValor = 0, maior = 0;
		boolean existeMaior = false;
		System.out.println("Informe o primeiro valor:");
		primeiroValor = entradaUsuario.nextInt();
		System.out.println("Informe o segundo valor:");
		segundoValor = entradaUsuario.nextInt();
		if (primeiroValor > segundoValor) {
			maior = primeiroValor;
		}
	}
}
